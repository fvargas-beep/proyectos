<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>servlets.index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>analisis</servlet-name>
        <servlet-class>servlets.analisis</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>enviarPDF</servlet-name>
        <servlet-class>servlets.enviarPDF</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bitacoraXML</servlet-name>
        <servlet-class>servlets.bitacoraXML</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>servlets.admin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>usuario</servlet-name>
        <servlet-class>servlets.usuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usuario</servlet-name>
        <url-pattern>/usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>analisis</servlet-name>
        <url-pattern>/analisis</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>enviarPDF</servlet-name>
        <url-pattern>/enviarPDF</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bitacoraXML</servlet-name>
        <url-pattern>/bitacoraXML</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
